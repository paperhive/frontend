{
  "name": "paperhive-frontend",
  "version": "1.0.0",
  "description": "PaperHive frontend",
  "scripts": {
    "build": "gulp",
    "install-deps": "npm install && bower install",
    "lint": "find src/js/ -name *.ts | xargs ./node_modules/.bin/tslint",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "karma start --single-run",
    "test:e2e": "gulp test"
  },
  "author": "Nico Schl√∂mer <nico@paperhive.org>",
  "license": "GPL-3.0",
  "private": true,
  "devDependencies": {
    "bower": "^1.6.5",
    "browserify": "^13.0.0",
    "browserify-shim": "^3.8.11",
    "connect-history-api-fallback": "^1.1.0",
    "del": "^2.0.2",
    "fs": "^0.0.2",
    "gulp": "^3.9.0",
    "gulp-angular-templatecache": "^1.8.0",
    "gulp-batch": "^1.0.5",
    "gulp-cachebust": "^0.0.6",
    "gulp-connect": "^2.2.0",
    "gulp-eslint": "^1.1.0",
    "gulp-htmlhint": "^0.3.0",
    "gulp-htmlmin": "^1.2.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.1",
    "gulp-protractor": "^2.0.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-streamify": "^1.0.2",
    "gulp-template": "^3.0.0",
    "gulp-typescript": "^2.10.0",
    "gulp-uglify": "^1.4.2",
    "gulp-util": "^3.0.7",
    "jasmine": "^2.4.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.15",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "merge-stream": "^1.0.0",
    "phantomjs": "^1.9.19",
    "tslint": "^3.3.0",
    "typescript": "^1.7.5",
    "typings": "^0.6.3",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.6.0"
  },
  "browser": {
    "angular-animate": "./bower_components/angular-animate/angular-animate.js",
    "angular-bootstrap-tpls": "./bower_components/angular-bootstrap/ui-bootstrap-tpls.js",
    "angular-leaflet-directive": "./bower_components/angular-leaflet-directive/dist/angular-leaflet-directive.js",
    "angular-moment": "./bower_components/angular-moment/angular-moment.js",
    "angular-route": "./bower_components/angular-route/angular-route.js",
    "angular-route-segment": "./bower_components/angular-route-segment/build/angular-route-segment.js",
    "angular-sanitize": "./bower_components/angular-sanitize/angular-sanitize.js",
    "detect-element-resize": "./bower_components/javascript-detect-element-resize/jquery.resize.js",
    "highlightjs": "./bower_components/highlightjs/highlight.pack.js",
    "leaflet": "./bower_components/leaflet/dist/leaflet.js",
    "pdfjs": "./bower_components/pdfjs-dist/build/pdf.js",
    "pdfjs-compatibility": "./bower_components/pdfjs-dist/web/compatibility.js",
    "mentio": "./bower_components/ment.io/dist/mentio.js",
    "kramed": "./bower_components/kramed/lib/kramed.js",
    "moment": "./bower_components/moment/moment.js",
    "ngSmoothScroll": "./bower_components/ngSmoothScroll/lib/angular-smooth-scroll.js",
    "rangy": "./bower_components/rangy/rangy-core.js",
    "rangy-classapplier": "./bower_components/rangy/rangy-classapplier.js",
    "rangy-highlighter": "./bower_components/rangy/rangy-highlighter.js",
    "rangy-serializer": "./bower_components/rangy/rangy-serializer.js"
  },
  "browserify-shim": {
    "angular-animate": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-bootstrap-tpls": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-leaflet-directive": {
      "exports": null,
      "depends": [
        "angular",
        "leaflet:L"
      ]
    },
    "angular-moment": {
      "exports": "angular-moment",
      "depends": [
        "angular",
        "moment"
      ]
    },
    "angular-route": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-route-segment": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "angular-sanitize": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "detect-element-resize": {
      "depends": [
        "jquery:jQuery"
      ]
    },
    "leaflet": {
      "exports": "L"
    },
    "moment": {
      "exports": "moment"
    },
    "ngSmoothScroll": {
      "exports": null,
      "depends": [
        "angular"
      ]
    },
    "pdfjs": {
      "exports": "PDFJS"
    },
    "highlightjs": {
      "exports": "hljs"
    }
  },
  "dependencies": {
    "angular": "^1.4.9",
    "jquery": "^2.2.0",
    "lodash": "^3.10.1",
    "paperhive-sources": "^4.0.1"
  }
}
