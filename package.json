{
  "name": "paperhive-frontend",
  "version": "1.0.0",
  "description": "PaperHive frontend",
  "scripts": {
    "build": "rm -rf build/* && mkdirp build && npm-run-all -s build:prod:*",
    "build:prod:css": "lessc --clean-css=\"--skip-restructuring\" less/index.less build/index.css",
    "build:prod:index": "gulp index",
    "build:prod:js": "gulp html && jspm build app/index.ts build-tmp/index.js --skip-rollup --skip-source-maps && uglifyjs build-tmp/index.js -m -c -o build/index.js",
    "build:prod:static": "gulp static",
    "build:prod:vendor": "gulp vendor",
    "build:dev:css": "lessc --source-map less/index.less build-dev/index.css",
    "build:dev:html": "gulp html --dev",
    "build:dev:index": "gulp index --dev",
    "build:dev:vendor": "gulp vendor --dev",
    "clean": "rm -rf build*",
    "install-deps": "npm install && bower install && jspm install && typings install",
    "lint:ts": "find app/ -name *.ts | xargs ./node_modules/.bin/tslint",
    "lint:js": "eslint test",
    "lint": "npm run lint:ts && npm run lint:js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "karma start --single-run",
    "test:e2e": "protractor protractor.conf.js",
    "watch": "rm -rf build-dev/* && mkdirp build-dev && npm-run-all build:dev:* && npm-run-all -p watch:*",
    "watch:css": "nodemon -C -e less -w less -x \"npm run build:dev:css\"",
    "watch:html": "nodemon -C -e html -w html -x \"npm run build:dev:html\"",
    "watch:index": "nodemon -C -e html -w index.template.html -x \"npm run build:dev:index\"",
    "watch:js": "jspm bundle app/index.ts build-dev/bundle.js -wd",
    "watch:serve": "live-server --no-browser --wait=50 --watch=build-dev  --mount=/:build-dev --entry-file=build-dev/index.html"
  },
  "author": "Andr√© Gaul <andre@paperhive.org>",
  "license": "GPL-3.0",
  "private": true,
  "devDependencies": {
    "bower": "^1.6.5",
    "co": "^4.6.0",
    "del": "^2.0.2",
    "eslint": "^3.0.1",
    "gulp": "^3.9.0",
    "gulp-angular-templatecache": "^2.0.0",
    "gulp-cachebust": "^0.0.6",
    "gulp-connect": "^5.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-imagemin": "^3.0.2",
    "gulp-streamify": "^1.0.2",
    "gulp-template": "^4.0.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "jasmine": "^2.4.0",
    "jasmine-core": "^2.3.4",
    "jspm": "^0.17.0-beta.28",
    "karma": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "less": "^2.6.0",
    "less-plugin-clean-css": "^1.5.1",
    "live-server": "^1.0.0",
    "lodash": "^4.3.0",
    "merge-stream": "^1.0.0",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.8.1",
    "npm-run-all": "^3.0.0",
    "phantomjs-prebuilt": "^2.1.3",
    "protractor": "^4.0.9",
    "tslint": "^3.3.0",
    "typescript": "^1.7.5",
    "typings": "^1.0.2",
    "uglify-js": "^2.6.1",
    "yargs": "^5.0.0"
  },
  "jspm": {
    "dependencies": {
      "Leaflet/Leaflet": "github:Leaflet/Leaflet@^0.7.7",
      "angular-route-segment": "npm:angular-route-segment@^1.5.1",
      "angulartics-google-analytics": "npm:angulartics-google-analytics@^0.2.1",
      "async": "npm:async@^2.0.1",
      "components/highlightjs": "github:components/highlightjs@^9.6.0",
      "core-js": "npm:core-js@^2.4.0",
      "dentist": "npm:dentist@^1.0.3",
      "highlightjs": "github:components/highlightjs@^9.6.0",
      "javascript-detect-element-resize": "github:sdecima/javascript-detect-element-resize@^0.5.3",
      "kramed": "npm:kramed@^0.5.6",
      "leaflet": "github:Leaflet/Leaflet@^0.7.7",
      "angular-animate": "github:angular/bower-angular-animate@^1.5.8",
      "angular-bootstrap": "github:angular-ui/bootstrap-bower@^2.1.2",
      "angular-leaflet-directive": "github:tombatossals/angular-leaflet-directive@^0.10.0",
      "angular-moment": "npm:angular-moment@^1.0.0-beta.6",
      "angular-route": "github:angular/bower-angular-route@^1.5.8",
      "angular-sanitize": "github:angular/bower-angular-sanitize@^1.5.8",
      "jquery": "npm:jquery@^3.1.0",
      "lodash": "npm:lodash@^4.15.0",
      "mozilla/pdfjs-dist": "github:mozilla/pdfjs-dist@^1.5.415",
      "paperhive-sources": "npm:paperhive-sources@^4.0.2",
      "pdfjs-dist": "github:mozilla/pdfjs-dist@^1.5.389",
      "rangy": "github:timdown/rangy-release@^1.3.0",
      "rx": "npm:rx@^4.1.0",
      "socket.io-client": "github:socketio/socket.io-client@^1.4.5"
    },
    "devDependencies": {
      "json": "github:systemjs/plugin-json@^0.1.2",
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.15",
      "text": "github:systemjs/plugin-text@^0.0.8",
      "ts": "github:frankwallis/plugin-typescript@^5.1.2"
    },
    "peerDependencies": {
      "angular": "github:angular/bower-angular@^1.5.8",
      "angulartics": "npm:angulartics@^1.1.3",
      "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
      "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
      "child_process": "github:jspm/nodelibs-child_process@^0.2.0-alpha",
      "constants": "github:jspm/nodelibs-constants@^0.2.0-alpha",
      "crypto": "github:jspm/nodelibs-crypto@^0.2.0-alpha",
      "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
      "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
      "os": "github:jspm/nodelibs-os@^0.2.0-alpha",
      "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
      "process": "github:jspm/nodelibs-process@^0.2.0-alpha",
      "readline": "github:jspm/nodelibs-readline@^0.2.0-alpha",
      "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
      "string_decoder": "github:jspm/nodelibs-string_decoder@^0.2.0-alpha",
      "url": "github:jspm/nodelibs-url@^0.2.0-alpha",
      "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
      "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha"
    },
    "overrides": {
      "github:Leaflet/Leaflet@0.7.7": {
        "dependencies": {
          "css": "*"
        },
        "meta": {
          "dist/leaflet-src.js": {
            "deps": [
              "./leaflet.css!"
            ],
            "format": "global"
          }
        }
      },
      "github:angular/bower-angular-animate@1.5.8": {
        "main": "angular-animate",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.3.0"
        },
        "meta": {
          "angular-animate.js": {
            "deps": [
              "angular"
            ]
          }
        }
      },
      "github:angular/bower-angular-route@1.5.8": {
        "main": "angular-route",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.3.0"
        },
        "meta": {
          "angular-route.js": {
            "deps": [
              "angular"
            ]
          }
        }
      },
      "github:angular/bower-angular-sanitize@1.5.8": {
        "main": "angular-sanitize",
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.3.0"
        },
        "meta": {
          "angular-sanitize.js": {
            "deps": [
              "angular"
            ]
          }
        }
      },
      "github:angular/bower-angular@1.5.8": {
        "main": "angular",
        "meta": {
          "angular.js": {
            "exports": "angular",
            "format": "global",
            "globals": {
              "jQuery": "jquery"
            }
          }
        }
      },
      "github:mozilla/pdfjs-dist@1.5.415": {
        "meta": {
          "web/compatibility.js": {
            "format": "global"
          }
        },
        "map": {}
      },
      "github:sdecima/javascript-detect-element-resize@0.5.3": {
        "main": "jquery.resize",
        "dependencies": {
          "jquery": "^3.1.0"
        },
        "meta": {
          "jquery.resize.js": {
            "globals": {
              "jQuery": "jquery"
            }
          }
        }
      },
      "github:tombatossals/angular-leaflet-directive@0.10.0": {
        "dependencies": {
          "leaflet": "^0.7.7"
        },
        "meta": {
          "dist/angular-leaflet-directive.js": {
            "globals": {
              "L": "leaflet"
            }
          }
        },
        "format": "detect",
        "map": {}
      },
      "github:timdown/rangy-release@1.3.0": {
        "format": "global",
        "main": "rangy-core",
        "meta": {
          "rangy-core.js": {
            "exports": "rangy"
          },
          "rangy-cssclassapplier.js": {
            "deps": [
              "./rangy-core"
            ]
          },
          "rangy-selectionsaverestore.js": {
            "deps": [
              "./rangy-core"
            ]
          },
          "rangy-serializer.js": {
            "deps": [
              "./rangy-core"
            ]
          }
        }
      },
      "npm:inherits@2.0.3": {
        "ignore": [
          "test.js"
        ]
      },
      "npm:jquery@3.1.0": {
        "format": "amd"
      },
      "npm:lodash@4.15.0": {
        "map": {
          "buffer": "@empty",
          "process": "@empty"
        }
      },
      "npm:moment@2.14.1": {
        "jspmNodeConversion": false
      },
      "npm:typescript@2.0.3": {
        "browser": {},
        "map": {
          "buffer": "@empty",
          "child_process": "@empty",
          "fs": "@empty",
          "path": "@empty",
          "process": "@empty",
          "readline": "@empty"
        }
      }
    }
  },
  "dependencies": {}
}
